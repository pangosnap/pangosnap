    "start": "next start",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:stylelint": "stylelint '**/*.{css,scss}' --fix --allow-empty-input",
    "lint": "pnpm run lint:eslint && pnpm run lint:stylelint",
