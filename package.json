    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "precommit": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{css,scss}": [
      "stylelint --fix --allow-empty-input"
    ]
  },
